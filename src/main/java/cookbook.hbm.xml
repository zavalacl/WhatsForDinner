<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
 <hibernate-mapping>
 		<class name="com.zavala.whatsfordinner.Cookbook" table="cookbook">
 		<id name ="cbID" column="cbID" type="int">
 			<generator class="increment"></generator>
 		</id>
 		<property name="recLabel" column="recLabel" type="string"></property>
 		<property name="recImage" column="recImage" type="string"></property>
 		<property name="recIngredients" column="recIngredients" type ="string"></property>
 		<property name="customerID" column="customerID" type ="int"></property>
 		<property name="recSource" column="recSource" type="string"></property>
 		<property name="recURL" column="recURL" type="string"></property>
 		
 		</class>
 </hibernate-mapping>